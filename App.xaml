<Application x:Class="Synapse.OptionLoader.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:conv="clr-namespace:Ecng.Xaml.Converters;assembly=Ecng.Xaml"
             xmlns:snpxaml="clr-namespace:Synapse.Xaml;assembly=Synapse.Xaml"
             xmlns:local="clr-namespace:Synapse.OptionLoader"
             Startup="App_Startup">
    <Application.Resources>
            <conv:InverseBooleanConverter x:Key="InverseBooleanConv"/>
            <conv:BoolToVisibilityConverter x:Key="BoolToVisibilityConv"/>
            <snpxaml:ProcessStatesImageConverter x:Key="ProcessStatesImageConv"/>
            <snpxaml:ConnectionStateImageConverter x:Key="ConnectionStateImageConv"/>
            <snpxaml:NullBoolConverter x:Key="NullBoolConv"/>
            <snpxaml:ProcessStateBoolConverter x:Key="ProcessStateBoolConv"/>
            <snpxaml:ProcessStatesRunImageConverter x:Key="ProcessStatesRunImageConv"/>
            <snpxaml:MultiParamConverter x:Key="MultiParamConv"/>
    </Application.Resources>
</Application>
